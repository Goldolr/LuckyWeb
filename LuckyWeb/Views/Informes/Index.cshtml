@model IEnumerable<LuckyWeb.Models.Informe>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Detalle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FK_PruebaInforme)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Detalle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FK_PruebaInforme.IDprueba)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IDinforme">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IDinforme">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IDinforme">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
