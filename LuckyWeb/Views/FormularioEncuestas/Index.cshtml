@model IEnumerable<LuckyWeb.Models.FormularioEncuesta>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FK_UserFormularioEncuesta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FK_MascotaFormularioEncuesta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FK_PreguntaFormularioEncuesta)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FK_UserFormularioEncuesta.IdUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FK_MascotaFormularioEncuesta.Esterilizado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FK_PreguntaFormularioEncuesta.IDpreguntas)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IDformularioEncuesta">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IDformularioEncuesta">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IDformularioEncuesta">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
